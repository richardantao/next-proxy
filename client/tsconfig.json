{
	"compilerOptions": {
		"target": "ES5",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"downlevelIteration": true,
		"noUncheckedIndexedAccess": true,
		"baseUrl": ".",
		"paths": {
			"*": [
				"/*"
			],
			"~/*": [
				"src/lib/*/index.ts"
			]
		}
	},
	"include": [
		"next-env.d.ts",
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}